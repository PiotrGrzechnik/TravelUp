{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./src/custom_typings", "./node_modules/@types"],
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"]
    }
  }
}
